package com.ufg.notificacoes.bean;

public class Disciplina {
	
	private Long id;
	private String nome;
	private Boolean receberNotificacoes
	
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
}
